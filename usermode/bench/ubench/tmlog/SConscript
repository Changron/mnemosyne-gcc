Import('ubenchEnv')
Import('mcoreLibrary')
Import('mtmLibrary')

myEnv = ubenchEnv.Clone() 
myEnv.Append(CPPPATH = ['#library/common'])


sources = Split("""
                main.c
                """)


myEnv.Append(LIBS = [mcoreLibrary])
myEnv.Append(LIBS = [mtmLibrary])
myEnv.Program('tmlog', sources)
